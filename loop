#include <stdio.h>

int main() {
    
    int c;

    for(c=10; c>0; c--)
    { printf("Hello\n");
    }

    return 0;
}




// Table creation using input from user

#include <stdio.h>

int main() {
    
    int n,c;
    printf("Please Input the Value of n:\n");
    scanf("%d", &n);

    for(c=1; c<=10; c++)
    { printf("%d*%d=%d\n",n,c,n*c);
    }

    return 0;
}


// sum of forst 10 number

#include <stdio.h>

int main() {
    
    int sum=0,c;

    for(c=0; c<=10; c++)
    { sum = sum + c;
    }
    printf("%d",sum);

    return 0;
}
